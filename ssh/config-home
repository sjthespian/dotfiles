# Defaults for all hosts
KeepAlive yes
Compression yes

Host asterisk.lapseofthought.com
Host asterisk
        User root
        ForwardAgent yes
	ForwardX11 yes

Host pdi
	Hostname ftp.pdi.com
        Compression yes
        ForwardAgent yes
        ForwardX11 yes
        GatewayPorts yes
        LocalForward 4025 localhost:4025
        LocalForward 4993 localhost:4993
        LocalForward 5900 localhost:5900

Host cowbell
	Hostname cowbell.employees.org
	IdentityFile ~/.ssh/id_dsa

# Send all other fqdn through pavo as a proxy
Host    *.*
	IdentityFile ~/.ssh/id_dsa
	#ProxyCommand ssh action.pdi.com nc %h %p
	ProxyCommand ssh pavo.anim.dreamworks.com nc %h %p
	ForwardAgent yes
	ForwardX11 yes
	Protocol 2,1
	ServerAliveInterval 1800
	Compression yes
# Defaults for all hosts
